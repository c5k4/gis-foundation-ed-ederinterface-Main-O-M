<UserControl x:Class="ArcFMSilverlight.PONSDialog"
   xmlns:sdk="http://schemas.microsoft.com/winfx/2006/xaml/presentation/sdk" 
   xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
   xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
   xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
   xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
   xmlns:controls="clr-namespace:System.Windows.Controls;assembly=System.Windows.Controls"
   mc:Ignorable="d" xmlns:toolkit="http://schemas.microsoft.com/winfx/2006/xaml/presentation/toolkit">
    <Grid x:Name="Customergrid">
        <StackPanel Name ="CustWiz" VerticalAlignment="Stretch" HorizontalAlignment="Stretch" >
            <StackPanel Name="tabItem1" Height="440" Width="650" Visibility="Visible">
                <Border BorderThickness="3" BorderBrush="#FF4EB81E" Height="420" Width="630" CornerRadius="10" HorizontalAlignment="Center" VerticalAlignment="Center">
                    <Grid Name="grid1">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="240"/>
                            <RowDefinition Height="50" />
                            <RowDefinition Height="120" />
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Vertical" VerticalAlignment="Center" Margin="0,50,0,0" Grid.Row="0" >
                            <Image Height="200" HorizontalAlignment="Center" Name="image1" Stretch="Uniform" VerticalAlignment="Top" Width="200" Source="/ArcFMSilverlight;component/Images/ill-sc1-opt4.png" />
                        </StackPanel>
                        <StackPanel Orientation="Vertical" VerticalAlignment="Center" Grid.Row="1" Margin="0,20,0,0">
                            <TextBlock Margin="10,0,10,0" HorizontalAlignment="Left"  TextWrapping="Wrap" Text="The Customer Notification Wizard will walk you through the steps required to identify and print a list of all customers specified for the notification of a planned outage." />
                        </StackPanel>
                        <StackPanel Orientation="Vertical" VerticalAlignment="Center" Grid.Row="2" >
                            <Grid  >
                                <Grid.RowDefinitions>
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                    <RowDefinition Height="30" />
                                </Grid.RowDefinitions>
                                <Grid.ColumnDefinitions>
                                    <ColumnDefinition Width="5.3*" />
                                    <ColumnDefinition Width="9*" />
                                </Grid.ColumnDefinitions>
                                <!--<TextBlock Grid.Row="0" Grid.Column ="0" Margin="10,0,0,0" VerticalAlignment="Center" HorizontalAlignment="Left" Text=
                                Select the Division and " TextAlignment="Left" Width="360">Click  <Span FontWeight="Bold">"Proceed"</Span> to continue</TextBlock>-->
                                <TextBlock Grid.Row="0" Grid.Column ="1" VerticalAlignment="Center" HorizontalAlignment="Left"  Name="textBlock6" Text="Select Division:"  Width="85" TextAlignment="Left"  />
                                <ComboBox Grid.Row="1" Style="{StaticResource PONSComboBox}" TabIndex="1"  Grid.Column ="1"  VerticalAlignment="Center" HorizontalAlignment="Left"  x:Name="dddivisionSearch" SelectionChanged="dddivisionSearch_SelectionChanged" Width="175" />
                                <Button x:Name="CancelButton" Style="{StaticResource PONSButton}" TabIndex="2" Grid.Row="2" Margin="0,5,0,0" Grid.Column ="1"  VerticalAlignment="Center" HorizontalAlignment="Left"  Width="75" Height="23"  Click="btnCancelTab1_Click">
                                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">

                                        <TextBlock Text="Close" TextAlignment="Center" Width="40" VerticalAlignment="Center"></TextBlock>
                                        <Image Source="/Images/close.png" Width="20"  />
                                    </StackPanel>
                                </Button>
                                <Button x:Name="OKButton" Style="{StaticResource PONSButton}" TabIndex="3" Grid.Row="2" Grid.Column ="1" Margin="90,5,0,0"  VerticalAlignment="Center" HorizontalAlignment="Left"  Click="OKButton_Click"  Width="85" Height="23" AllowDrop="True">
                                    <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                        <TextBlock Text="Proceed" TextAlignment="Center"  Width="45" VerticalAlignment="Center"></TextBlock>
                                        <Image Source="/Images/next.png" Width="20" Margin="2,0,0,0" />
                                    </StackPanel>
                                </Button>


                            </Grid>

                        </StackPanel>
                    </Grid>
                </Border>
            </StackPanel>
            <!--Search Options-->
            <StackPanel Name="tabItem2" Height="440" Width="650" Visibility="Collapsed">
                <Border BorderThickness="3"  BorderBrush="#FF4EB81E" Height="420" Width="630" HorizontalAlignment="Center" VerticalAlignment="Center" CornerRadius="10">
                    <toolkit:BusyIndicator HorizontalAlignment="Center" VerticalAlignment="Center" Name="busyIndicator" IsBusy="False">
                        <Grid Name="grid2" >
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1" />
                                <RowDefinition Height="105" />
                                <RowDefinition Height="45" />
                                <RowDefinition Height="30" />
                                <RowDefinition Height="20" />
                                <RowDefinition Height="175" />
                                <RowDefinition Height="130"/>
                            </Grid.RowDefinitions>
                            <StackPanel Orientation="Vertical"  Height="210" VerticalAlignment="Top" Grid.Row="1" Margin="10,10,0,0">
                                <TextBlock TextWrapping="Wrap" Text="On the screen you define the Searches that will identify the customers to be notified." Width="500" HorizontalAlignment="Left" ></TextBlock>
                                <RadioButton Style="{StaticResource PONSRadioButton}" Height="18" TabIndex="0" Content="Customer beyond or between source side devices" IsChecked="True"  Checked="RadioButton_Checked" x:Name="RdBetweenDevice" GroupName="SearchDevice"   />
                                <RadioButton Style="{StaticResource PONSRadioButton}" Height="18" TabIndex="1" Content="Customer served by a specific transformer" Checked="RadioButton_Checked" x:Name="RdTranformer" GroupName="SearchDevice"  />
                                <RadioButton Style="{StaticResource PONSRadioButton}" Height="18" TabIndex="2" Content="Customer by Substation or circuit" Checked="RadioButton_Checked" x:Name="RdCircuit" GroupName="SearchDevice"  />
                                <RadioButton Style="{StaticResource PONSRadioButton}" Height="18" Width="200" HorizontalAlignment="Left"  TabIndex="3" Content="Select Customers by Polygon" Checked="RadioButton_Checked" x:Name="RdSelectMap" GroupName="SearchDevice"  />
                            </StackPanel>
                            <StackPanel Name="DeviceEntry" Orientation="Vertical" VerticalAlignment="Top" Grid.Row="2" Visibility="Visible" Margin="10,0,0,0">
                                <StackPanel Orientation="Horizontal" Visibility="Visible" HorizontalAlignment="Center" Name="BtnBeyond" >
                                    <TextBlock  VerticalAlignment="Center" >Start Device:</TextBlock>
                                    <TextBox TabIndex="3" Name="txt_StartDevice" KeyUp="CustWiz_KeyUp" Text="" Width="100" Margin="5,0,0,0" ></TextBox>
                                    <TextBlock   VerticalAlignment="Center" Margin="10,0,0,0" >End Device:</TextBlock>
                                    <TextBox TabIndex="4" Name="txt_EndDevice" KeyUp="CustWiz_KeyUp" Text="" Width="100" Margin="5,0,0,0" ></TextBox>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Name="DeviceEntryTransformer" VerticalAlignment="Top" Grid.Row="2" Visibility="Collapsed"  >
                                <StackPanel Orientation="Horizontal" Name="stacktrans" VerticalAlignment="Center" Visibility="Visible" HorizontalAlignment="Center">
                                    <TextBlock VerticalAlignment="Center" HorizontalAlignment="Center">Transformer CGC or TNum :</TextBlock>
                                    <TextBox Name="txtTransformer" TabIndex="5" KeyUp="CustWiz_KeyUp" Text="" Width="120"  HorizontalAlignment="Left" VerticalAlignment="Center"></TextBox>
                                </StackPanel>
                            </StackPanel>
                            <StackPanel Name="stackcircuit" Orientation="Vertical" VerticalAlignment="Top" Grid.Row="2" Visibility="Collapsed" Margin="10,0,0,0" >
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center" Name="stackcircuit1" Visibility="Visible" >
                                    <Grid Name="grid22" Grid.Row="3" >
                                        <Grid.RowDefinitions>
                                            <RowDefinition Height="*" />
                                        </Grid.RowDefinitions>
                                        <Grid.ColumnDefinitions>
                                            <ColumnDefinition Width="70" />
                                            <ColumnDefinition Width="200" />
                                            <ColumnDefinition Width="40" />
                                            <ColumnDefinition Width="90" />
                                            <ColumnDefinition Width="50" />
                                            <ColumnDefinition Width="110" />
                                        </Grid.ColumnDefinitions>
                                        <!--<TextBlock Height="22" HorizontalAlignment="Left" VerticalAlignment="Top" Width="110" Text="Division: " TextWrapping="NoWrap" x:Name="Division" TextAlignment="Left" Margin="0,2,0,0" Grid.Row="3" />
                                    <ComboBox Height="22" Width="150" Margin="-50,2,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"  x:Name="dddivision" SelectionChanged="dddivision_SelectionChanged" Grid.Row="3"  />-->
                                        <TextBlock Grid.Row="0" Grid.Column="0" HorizontalAlignment="Left" VerticalAlignment="Center"  Text="Substation:"  x:Name="substaion"  />
                                        <ComboBox Style="{StaticResource PONSComboBox}"  Grid.Row="0" TabIndex="6" Grid.Column="1" Width="180"  HorizontalAlignment="Left" VerticalAlignment="Center"  x:Name="ddsubstation" SelectionChanged="ddsubstation_SelectionChanged" />
                                        <TextBlock  Grid.Row="0" Grid.Column="2"  HorizontalAlignment="Left" VerticalAlignment="Center" Width="40" Text="Bank:"  x:Name="bank"  />
                                        <ComboBox Style="{StaticResource PONSComboBox}" KeyUp="CustWiz_KeyUp" Grid.Row="0" TabIndex="7" Grid.Column="3" Width="80"  HorizontalAlignment="Left" VerticalAlignment="Center" x:Name="ddbank" SelectionChanged="ddbank_SelectionChanged"/>
                                        <TextBlock Grid.Row="0" Grid.Column="4"  HorizontalAlignment="Left" VerticalAlignment="Center" Width="60" Text="Circuit:"  x:Name="circuit"  />
                                        <ComboBox Style="{StaticResource PONSComboBox}" KeyUp="CustWiz_KeyUp" Grid.Row="0" TabIndex="8" Grid.Column="5" Width="110"  HorizontalAlignment="Left" VerticalAlignment="Center"  x:Name="ddcircuit" SelectionChanged="ddcircuit_SelectionChanged"  />
                                    </Grid>

                                </StackPanel>
                            </StackPanel>
                            <Button x:Name="SearchCancelButton" Style="{StaticResource PONSButton}" TabIndex="9"  Click="SearchCancelButton_Click" Width="80"  Height="23" HorizontalAlignment="Center" Grid.Row="3" VerticalAlignment="Center" Margin="-100,-10,0,0">
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                    <TextBlock Text="Clear" TextAlignment="Center"  Width="45" VerticalAlignment="Center"></TextBlock>
                                    <Image Source="/Miner.Server.Client.Toolkit;component/Images/eraser.png" Width="15"  />
                                </StackPanel>
                            </Button>
                            <Button x:Name="DeviceButton" TabIndex="10" Style="{StaticResource PONSButton}" Click="DeviceButton_Click" Width="75" Height="23" HorizontalAlignment="Center" Grid.Row="3" VerticalAlignment="Center" Margin="100,-10,0,0">
                                <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                    <TextBlock Text="OK" TextAlignment="Center"  Width="35" VerticalAlignment="Center"></TextBlock>
                                    <Image Source="/Images/ok.png" Width="20"  />
                                </StackPanel>
                            </Button>

                            <StackPanel Orientation="Vertical" VerticalAlignment="Top" Grid.Row="4">
                                <TextBlock TextWrapping="Wrap"  VerticalAlignment="Bottom" Height="20" Text="Please select search result from the result list." HorizontalAlignment="Left" Width="500" Margin="10,0,0,0"></TextBlock>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" VerticalAlignment="Top" Grid.Row="5">
                                <CheckBox x:Name="chk_serviceAddress" Style="{StaticResource PONSCheckBox}" TabIndex="11" FontSize="10" FontWeight="Normal" Height="20" Margin="10,0,0,0" Width="Auto" VerticalAlignment="Center" IsChecked="False">
                                    Exclude service address from list if a mailing address is present</CheckBox>
                                <ScrollViewer x:Name="DataGridScrollViewer" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Visibility="Visible">
                                    <ListBox IsTabStop="False" Name="SearchlistDataGrid" SelectionChanged="SearchlistDataGrid_SelectionChanged" Height="140" Width="610" BorderThickness="2,2,2,3" SelectionMode="Multiple" HorizontalAlignment="Center" VerticalAlignment="Center">
                                        <ListBox.ItemTemplate>
                                            <DataTemplate>
                                                <StackPanel>
                                                    <TextBlock FontFamily="Verdana" Name="DeviceN" Text="{Binding DeviceName}" ></TextBlock>
                                                    <TextBlock FontFamily="Verdana" Name="DeviceID" Text="{Binding DeviceID}" Visibility="Collapsed" ></TextBlock>
                                                </StackPanel>
                                            </DataTemplate>
                                        </ListBox.ItemTemplate>
                                    </ListBox>
                                </ScrollViewer>
                            </StackPanel>

                            <StackPanel Orientation="Vertical" Grid.Row="6" >
                                <StackPanel Orientation="Horizontal" HorizontalAlignment="Center">
                                    <Button Style="{StaticResource PONSButton}" x:Name="btnPre3" TabIndex="12"  Click="btnPre3_Click" Width="85" HorizontalAlignment="Center" Height="23" Margin="0,0,20,0">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <Image Source="/Images/previous.png" Width="20"  />
                                            <TextBlock Text="Previous" TextAlignment="Center"  Width="50" VerticalAlignment="Center"></TextBlock>
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="btnDeleteGetCustomer" Style="{StaticResource PONSButton}" TabIndex="13" Width="85" Height="23"  HorizontalAlignment="Center" Click="btnDeleteGetCustomer_Click" Margin="0,0,20,0">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <TextBlock Text="Delete" TextAlignment="Center"  Width="45" VerticalAlignment="Center"></TextBlock>
                                            <Image Source="/Images/delete.png" Width="20"  />
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="GetCustomer" TabIndex="14" Style="{StaticResource PONSButton}" Click="GetCustomer_Click" Width="80" Height="23" IsEnabled="False"  HorizontalAlignment='Center' Margin="0,0,20,0">
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <TextBlock Text="Next" TextAlignment="Center"  Width="45" VerticalAlignment="Center"></TextBlock>
                                            <Image Source="/Images/next.png" Width="20"  />
                                        </StackPanel>
                                    </Button>
                                    <Button x:Name="btnCloseResult" TabIndex="15" Style="{StaticResource PONSButton}" Width="80" Height="23"  HorizontalAlignment="Center" Click="btnCloseResult_Click" >
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <TextBlock Text="Close" TextAlignment="Center"  Width="40" VerticalAlignment="Center"></TextBlock>
                                            <Image Source="/Images/close.png" Width="20"  />
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </StackPanel>
                        </Grid>
                    </toolkit:BusyIndicator>
                </Border>
            </StackPanel>
            <!--Customer List-->
            <StackPanel Name="tabItem4" Visibility="Collapsed" Height="440" Width="650">
                <Border BorderThickness="3" BorderBrush="#FF4EB81E"  Height="420" Width="630"  HorizontalAlignment="Center" VerticalAlignment="Center" CornerRadius="10">
                    <toolkit:BusyIndicator HorizontalAlignment="Center" VerticalAlignment="Center" Name="busyIndicator_Customer" IsBusy="False" >
                        <Grid x:Name="gridcus">
                            <Grid.RowDefinitions>
                                <RowDefinition Height="1" />
                                <RowDefinition Height="60" />
                                <RowDefinition Height="280" />
                                <RowDefinition Height="46"></RowDefinition>
                                <RowDefinition Height="23*" />
                            </Grid.RowDefinitions>
                            <StackPanel Orientation="Vertical" Grid.Row="1" Margin="5,0,5,0" Height="60">
                                <TextBlock TextWrapping="Wrap"  Text="Only checked customers will be included and unchecked will be excluded from the customer list. When you are satisfied with list of customers click Next button to define the outage date and time."></TextBlock>
                                <!--<Image Height="87" Name="image4" Stretch="Uniform" Width="145" Source="/ArcFMSilverlight;component/Images/ill-sc5-opt4.png" VerticalAlignment="Center" HorizontalAlignment="Center" />-->
                            </StackPanel>
                            <StackPanel Grid.Row="2" Margin="5,0,5,0">
                                <TextBlock FontSize="10" FontWeight="Normal" HorizontalAlignment="Right" Name="MetCountText" Text="Total Number of Meters:" Margin="0,0,75,0"></TextBlock>
                                <TextBlock FontSize="10" FontWeight="Normal" HorizontalAlignment="Right" Name="CustCountText" Text="Total Number of Letters:" Margin="0,0,75,0"></TextBlock>
                                <TextBlock FontSize="10" FontWeight="Normal" HorizontalAlignment="Right" Name="MetCount" Text="0" Margin="0,-30,20,0" Width="50" TextAlignment="Center"></TextBlock>
                                <TextBlock FontSize="10" FontWeight="Normal" HorizontalAlignment="Right" Name="CustCount" Text="0" Margin="0,-15,20,0" Width="50" TextAlignment="Center"></TextBlock>
                                <CheckBox  x:Name="Chcountcustomer"   IsChecked="{Binding Path=Checked, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  Content="Select All"  Style="{StaticResource PONSCheckBox}" Click="Chcountcustomer_Click" Width="145" Margin="-460,0,0,2"></CheckBox>
                                <ScrollViewer IsTabStop="False" x:Name="DataGridScrollViewerCustRest" HorizontalScrollBarVisibility="Auto" VerticalScrollBarVisibility="Auto" Visibility="Visible">
                                    <sdk:DataGrid x:Name="CustomerDataGrid"    IsTabStop="False" AutoGenerateColumns="False" CanUserSortColumns="True"  Width="610"  MaxWidth="620" Height="230" RowHeight="20" BorderThickness="2" >
                                        <sdk:DataGrid.Columns>


                                            <sdk:DataGridTemplateColumn  Header="">
                                                <!--<sdk:DataGridTemplateColumn Width="80" HeaderStyle="{StaticResource DataGridColumnHeaderStyle}">-->

                                                <sdk:DataGridTemplateColumn.CellTemplate>

                                                    <DataTemplate>
                                                        <CheckBox x:Name="radioSelectdevice"  IsChecked="{Binding Path=Checked, Mode=TwoWay, UpdateSourceTrigger=PropertyChanged}"  VerticalAlignment="Center" HorizontalAlignment="Center" Click="radioSelectdevice_Click" />
                                                    </DataTemplate>
                                                </sdk:DataGridTemplateColumn.CellTemplate>
                                            </sdk:DataGridTemplateColumn>
                                            <!--<sdk:DataGridCheckBoxColumn CanUserReorder="True" CanUserResize="True" CanUserSort="True" Header="All" Width="Auto" />-->
                                            <sdk:DataGridTextColumn CanUserReorder="True" CanUserResize="True" CanUserSort="True" Header="Type" Width="Auto" Binding="{Binding CustType}" IsReadOnly="True" />
                                            <sdk:DataGridTextColumn CanUserReorder="True" CanUserResize="True" CanUserSort="True" Header="Meter Number" Width="Auto" Binding="{Binding MeterNum}" IsReadOnly="True"/>
                                            <sdk:DataGridTextColumn CanUserReorder="True" CanUserResize="True" CanUserSort="True" Header="Customer Name" Width="300" Binding="{Binding CustomerName}" IsReadOnly="True"/>
                                            <sdk:DataGridTextColumn CanUserReorder="True" CanUserResize="True" CanUserSort="True" Header="Customer Address" Width="Auto" Binding="{Binding GridADDCol}" IsReadOnly="True"/>
                                            <sdk:DataGridTextColumn CanUserReorder="True" CanUserResize="True" CanUserSort="True" Header="Service Point ID" Width="Auto" Binding="{Binding ServicepointId}" IsReadOnly="True"/>
                                        </sdk:DataGrid.Columns>
                                    </sdk:DataGrid>
                                </ScrollViewer>
                            </StackPanel>
                            <StackPanel Orientation="Vertical" VerticalAlignment="Center" Grid.Row="3" Margin="0,5,0,0" >
                                <StackPanel Orientation="Horizontal"  VerticalAlignment="Center" HorizontalAlignment="Center">
                                    <Button TabIndex="0" x:Name="btnPre4" Style="{StaticResource PONSButton}" Width="95" Height="23" Click="btnPre4_Click" Margin="0,0,20,0" >
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <Image Source="/Images/previous.png" Width="20"  />
                                            <TextBlock Text="Previous" Margin="3,0,0,0" TextAlignment="Center"  Width="50" VerticalAlignment="Center"></TextBlock>
                                        </StackPanel>
                                    </Button>
                                    <Button TabIndex="1" x:Name="btnNextsearchlist" Style="{StaticResource PONSButton}" Width="90" Height="23" Click="btnNextsearchlist_Click"  Margin="0,0,20,0" >
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <TextBlock Text="Next" Margin="3,0,0,0" TextAlignment="Center"  Width="50" VerticalAlignment="Center"></TextBlock>
                                            <Image Source="/Images/next.png" Width="20"  />
                                        </StackPanel>
                                    </Button>
                                    <Button TabIndex="2" x:Name="btnCloseCustResult" Style="{StaticResource PONSButton}" Width="80" Height="23" Click="btnCloseCustResult_Click" >
                                        <StackPanel Orientation="Horizontal" VerticalAlignment="Center">
                                            <TextBlock Text="Close" Margin="3,0,0,0" TextAlignment="Center"  Width="40" VerticalAlignment="Center"></TextBlock>
                                            <Image Source="/Images/close.png" Width="20"  />
                                        </StackPanel>
                                    </Button>
                                </StackPanel>
                            </StackPanel>

                        </Grid>
                    </toolkit:BusyIndicator>
                </Border>
            </StackPanel>
            <!--Outage Window-->
            <StackPanel Name="tabItem5" Visibility="Collapsed" Height="440" Width="650">
                <Border BorderThickness="3" BorderBrush="#FF4EB81E" Height="420" Width="630"  HorizontalAlignment="Center" VerticalAlignment="Center" CornerRadius="10" >
                    <Grid x:Name="gridoutage" Height="450">
                        <Grid.RowDefinitions>
                            <RowDefinition Height="10" />
                            <RowDefinition Height="50" />
                            <RowDefinition Height="10" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="60" />
                            <RowDefinition Height="30" />
                            <RowDefinition Height="60" />
                            <RowDefinition Height="10"/>
                            <RowDefinition Height="65" />
                            <RowDefinition Height="65" />
                            <RowDefinition Height="10" />
                        </Grid.RowDefinitions>
                        <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Margin="10,0,0,0" Grid.Row="1" >
                            <TextBlock TextWrapping="Wrap" HorizontalAlignment="Left">Specify the outage date and duration. The dates must be future Dates and time ON must be after the time OFF. If the Power will be out for two times spans or overnight, click on&#160;<Span FontWeight="Bold">"Add an additional....."</Span>&#160;check box and define the second outage date and times.</TextBlock>
                        </StackPanel>
                        <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Margin="10,0,0,0"  Grid.Row="3">
                            <TextBlock TextWrapping="Wrap" HorizontalAlignment="Center">When you are finished setting up the outage duration, click on the&#160;<Span FontWeight="Bold"> "Next" </Span>&#160;button to define PSC details.</TextBlock>
                        </StackPanel>
                        <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Margin="10,0,0,0" Grid.Row="4">
                            <TextBlock HorizontalAlignment="Left" >Print only one outage Date / Time on output</TextBlock>
                        </StackPanel>
                        <TextBlock Height="22" HorizontalAlignment="Left" VerticalAlignment="Top" Width="110" Text="Date of Shutdown:" TextWrapping="NoWrap" x:Name="declong" TextAlignment="Left" Margin="42,2,0,0" Grid.Row="5" />
                        <Border Grid.Row="5" BorderThickness="1" Width="100" Height="30" Margin="42,24,0,0" VerticalAlignment="Top" HorizontalAlignment="Left"></Border>
                        <TextBlock Height="22" Margin="217,2,241,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="56" Text="Time Off" TextWrapping="NoWrap" x:Name="declat" TextAlignment="Right" Grid.Row="5" />
                        <toolkit:TimePicker TabIndex="2" Grid.Row="5" Height="22" Margin="183,26,205,12" Name="timePickerFrstOff" HorizontalAlignment="Center" />
                        <TextBlock Height="22" Margin="366,2,92,0" HorizontalAlignment="Center" VerticalAlignment="Top" Width="56" Text="Time On" TextWrapping="NoWrap" x:Name="timeon" TextAlignment="Right" Grid.Row="5" />
                        <toolkit:TimePicker TabIndex="3" Grid.Row="5" Height="22" Margin="347,25,43,11" Name="timePickerFrstOn" Width="110" HorizontalAlignment="Center" />
                        <sdk:DatePicker TabIndex="1" Name="datePickerFstShutDateOff" Width="98" HorizontalAlignment="Stretch" HorizontalContentAlignment="Stretch" Margin="5,24,450,8" Grid.Row="5" />

                        <StackPanel Orientation="Vertical" Grid.Row="6">
                            <StackPanel Orientation="Horizontal">
                                <CheckBox Style="{StaticResource PONSCheckBox}" TabIndex="4" Height="30"  Name="AddDate" Margin="10,0,0,0" Checked="AddDate_Checked" IsChecked="False" >Add additional shutdown Date / Time</CheckBox>
                                <!--<CheckBox Height="30" IsChecked="False" Margin="5,0,0,0">Add additional shutdown Date / Time</CheckBox>-->
                            </StackPanel>
                        </StackPanel>

                        <!--<StackPanel Orientation="Vertical" MaxWidth="500" Grid.Row="7" Margin="20,0" Grid.RowSpan="2" Width="520" Height="100">
                            <sdk:DatePicker Name="datePickerScndShutDateOff" Width="98" Margin="90,27,0,5" IsEnabled="{Binding IsChecked, ElementName=AddDate}"/>
                        </StackPanel>-->

                        <TextBlock Grid.Row="7" Height="22" HorizontalAlignment="Left" Margin="41,10,0,0" Name="textBlock2" Text="Date of Shutdown:" TextAlignment="Left" TextWrapping="NoWrap" VerticalAlignment="Top" Width="110" />
                        <TextBlock Grid.Row="7" Height="22" HorizontalAlignment="Center" Margin="216,13,242,0" Name="textBlock3" Text="Time Off" TextAlignment="Right" TextWrapping="NoWrap" VerticalAlignment="Top" Width="56" />
                        <toolkit:TimePicker TabIndex="6" Grid.Row="7" Height="22" HorizontalAlignment="Center" Margin="182,28,207,10" Name="timePickerScndOff" IsEnabled="{Binding IsChecked, ElementName=AddDate}"/>
                        <TextBlock Grid.Row="7" Height="22" HorizontalAlignment="Center" Margin="370,13,88,0" Name="textBlock4" Text="Time On" TextAlignment="Right" TextWrapping="NoWrap" VerticalAlignment="Top" Width="56" />
                        <sdk:DatePicker TabIndex="5" Grid.Row="7" HorizontalAlignment="Left" Margin="43,24,0,5" Name="datePickerScndOff" Width="98" IsEnabled="{Binding IsChecked, ElementName=AddDate}"/>
                        <toolkit:TimePicker TabIndex="7" Grid.Row="7" Height="22" HorizontalAlignment="Center" Margin="344,25,45,7" Name="timePickerScndOn" Width="110" IsEnabled="{Binding IsChecked, ElementName=AddDate}"/>

                        <StackPanel Orientation="Vertical" HorizontalAlignment="Left" Margin="10,0,0,0" Width="610" Height="80" Grid.Row="9" >
                            <TextBlock Height="22" Text="Enter the description of affected Outage Area &amp; Feeder ID in digits:" TextWrapping="NoWrap" x:Name="Remarks" Grid.Row="7" Margin="0,10,0,0" />
                            <TextBox Height="40" TabIndex="8" Width="610" Margin="0" TextWrapping="Wrap" x:Name="txtoutagearea" Grid.Row="7" Padding="0,2,5,2" MaxLength="65" HorizontalContentAlignment="Stretch" />
                        </StackPanel>

                        <StackPanel Orientation="Vertical" Grid.Row="10" Margin="0,10,0,0">
                            <StackPanel Orientation="Horizontal" VerticalAlignment="Center" HorizontalAlignment="Center">
                                <Button TabIndex="9" x:Name="btnreset5" Style="{StaticResource PONSButton}" Click="btnreset5_Click"  Width="85" Height="23" Margin="0,0,20,0">
