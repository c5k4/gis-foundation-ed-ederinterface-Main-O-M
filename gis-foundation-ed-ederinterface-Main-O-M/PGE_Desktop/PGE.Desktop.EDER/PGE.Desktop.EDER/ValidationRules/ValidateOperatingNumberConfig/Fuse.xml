<?xml version="1.0"?>
<SymbolNumberRules xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
xmlns:xsd="http://www.w3.org/2001/XMLSchema">
  <FeatureClass Name="Fuse">
    <Rule BeginsWith="" EndsWith="1" MinLength="4" MaxLength="7" Required="1">
      <Or>
        <Attribute FieldName="InstallationType"><Value>OH</Value></Attribute>
        <Attribute FieldName="InstallationType"><Value>PAD</Value></Attribute>
      </Or>
    </Rule>
    <Rule BeginsWith="" EndsWith="0" MinLength="4" MaxLength="7" Required="1">
      <Or>
        <Attribute FieldName="InstallationType"><Value>UG</Value></Attribute>
        <Attribute FieldName="InstallationType"><Value>SUB</Value></Attribute>
      </Or>
    </Rule>
  </FeatureClass>
</SymbolNumberRules>