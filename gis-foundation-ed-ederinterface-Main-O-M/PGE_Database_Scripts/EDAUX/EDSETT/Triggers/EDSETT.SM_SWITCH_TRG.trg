Prompt drop Trigger SM_SWITCH_TRG;
DROP TRIGGER EDSETT.SM_SWITCH_TRG
/

Prompt Trigger SM_SWITCH_TRG;
--
-- SM_SWITCH_TRG  (Trigger) 
--
CREATE OR REPLACE TRIGGER EDSETT.SM_SWITCH_TRG
BEFORE INSERT ON EDSETT.SM_SWITCH
FOR EACH ROW
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    IF INSERTING AND :NEW.ID IS NULL THEN
      SELECT SM_SWITCH_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
    END IF;
  END COLUMN_SEQUENCES;
END;
/
