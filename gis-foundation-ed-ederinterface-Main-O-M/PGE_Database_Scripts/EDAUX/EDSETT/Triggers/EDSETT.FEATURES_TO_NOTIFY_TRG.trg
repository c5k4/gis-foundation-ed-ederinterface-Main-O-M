Prompt drop Trigger FEATURES_TO_NOTIFY_TRG;
DROP TRIGGER EDSETT.FEATURES_TO_NOTIFY_TRG
/

Prompt Trigger FEATURES_TO_NOTIFY_TRG;
--
-- FEATURES_TO_NOTIFY_TRG  (Trigger) 
--
CREATE OR REPLACE TRIGGER EDSETT.FEATURES_TO_NOTIFY_TRG
BEFORE INSERT ON EDSETT.FEATURES_TO_NOTIFY
FOR EACH ROW
BEGIN
  IF INSERTING AND :NEW.ID IS NULL THEN
    SELECT FEATURES_NOTIFICATION_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
  END IF;
END;
/
