Prompt Function INSERT_MONTHLY_LOAD_LOG;
--
-- INSERT_MONTHLY_LOAD_LOG  (Function) 
--
CREATE OR REPLACE FUNCTION EDTLM."INSERT_MONTHLY_LOAD_LOG" (TABLE_NAME VARCHAR2) RETURN VARCHAR2 AS
BEGIN

