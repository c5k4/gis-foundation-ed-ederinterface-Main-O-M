PROMPT View SDO_DATUM_VERTICAL;
--
-- SDO_DATUM_VERTICAL  (View)
--

CREATE OR REPLACE FORCE VIEW MDSYS.SDO_DATUM_VERTICAL
(
    DATUM_ID,
    DATUM_NAME,
    ELLIPSOID_ID,
    PRIME_MERIDIAN_ID,
    INFORMATION_SOURCE,
    DATA_SOURCE,
    SHIFT_X,
    SHIFT_Y,
    SHIFT_Z,
    ROTATE_X,
    ROTATE_Y,
    ROTATE_Z,
    SCALE_ADJUST
)
AS
    SELECT DATUM_ID,
           DATUM_NAME,
           ELLIPSOID_ID,
           PRIME_MERIDIAN_ID,
           INFORMATION_SOURCE,
           DATA_SOURCE,
           SHIFT_X,
           SHIFT_Y,
           SHIFT_Z,
           ROTATE_X,
           ROTATE_Y,
           ROTATE_Z,
           SCALE_ADJUST
      FROM MDSYS.SDO_DATUMS
     WHERE DATUM_TYPE = 'VERTICAL'
/


Prompt Synonym SDO_DATUM_VERTICAL;
--
-- SDO_DATUM_VERTICAL  (Synonym) 
--
CREATE OR REPLACE PUBLIC SYNONYM SDO_DATUM_VERTICAL FOR MDSYS.SDO_DATUM_VERTICAL
/


Prompt Grants on VIEW SDO_DATUM_VERTICAL TO PUBLIC to PUBLIC;
GRANT SELECT ON MDSYS.SDO_DATUM_VERTICAL TO PUBLIC
