PROMPT View SDO_CRS_GEOGRAPHIC2D;
--
-- SDO_CRS_GEOGRAPHIC2D  (View)
--

CREATE OR REPLACE FORCE VIEW MDSYS.SDO_CRS_GEOGRAPHIC2D
(
    SRID,
    COORD_REF_SYS_NAME,
    COORD_SYS_ID,
    DATUM_ID,
    INFORMATION_SOURCE,
    DATA_SOURCE
)
AS
    SELECT SRID,
           COORD_REF_SYS_NAME,
           COORD_SYS_ID,
           DATUM_ID,
           INFORMATION_SOURCE,
           DATA_SOURCE
      FROM MDSYS.SDO_COORD_REF_SYS
     WHERE COORD_REF_SYS_KIND = 'GEOGRAPHIC2D'
/


Prompt Synonym SDO_CRS_GEOGRAPHIC2D;
--
-- SDO_CRS_GEOGRAPHIC2D  (Synonym) 
