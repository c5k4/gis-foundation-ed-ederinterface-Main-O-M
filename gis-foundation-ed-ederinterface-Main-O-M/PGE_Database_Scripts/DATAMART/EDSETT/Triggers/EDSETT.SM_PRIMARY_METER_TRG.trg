Prompt drop Trigger SM_PRIMARY_METER_TRG;
DROP TRIGGER EDSETT.SM_PRIMARY_METER_TRG
/

Prompt Trigger SM_PRIMARY_METER_TRG;
--
-- SM_PRIMARY_METER_TRG  (Trigger) 
--
CREATE OR REPLACE TRIGGER EDSETT.SM_PRIMARY_METER_TRG
BEFORE INSERT ON EDSETT.SM_PRIMARY_METER
FOR EACH ROW
BEGIN
  <<COLUMN_SEQUENCES>>
  BEGIN
    IF INSERTING AND :NEW.ID IS NULL THEN
      SELECT SM_PRIMARY_METER_SEQ.NEXTVAL INTO :NEW.ID FROM SYS.DUAL;
    END IF;
  END COLUMN_SEQUENCES;
END;
/
