           "SUMMER_LOAD_LIMIT",
           "WINTER_LOAD_LIMIT",
           "LIMITING_FACTOR",
           "OPERATING_MODE",
           "FLISR_ENGINEERING_COMMENTS"
      FROM SM_INTERRUPTER
     WHERE     DATE_MODIFIED >= SM_EXPOSE_DATA_PKG.GET_DATEFROM
           AND DATE_MODIFIED <= SM_EXPOSE_DATA_PKG.GET_DATETO
           AND CURRENT_FUTURE = 'C'
