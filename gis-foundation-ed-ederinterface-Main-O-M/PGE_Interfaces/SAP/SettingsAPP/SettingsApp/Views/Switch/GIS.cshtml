@using SettingsApp.Common;
@model SettingsApp.Models.SwitchModel
@using (Html.BeginForm())
{
    <table cellpadding="0" cellspacing="0" width="90%">
        <tr>
            <td style="padding: 0px">
                <div class="float-left">
                    <nav>
                        <ul id="menu">
                            <li class="selected">@Html.ActionLink("GIS", "GIS", "Switch", new { globalID = ViewBag.GlobalID, layerName = ViewBag.LayerName, layerType = ViewBag.layerType }, null)</li>
                            <li>@Html.ActionLink("Current Settings", "Index", "Switch", new { globalID = ViewBag.GlobalID, layerName = ViewBag.LayerName, layerType = ViewBag.layerType }, null)</li>
                            <li>@Html.ActionLink("Future Settings", "Future", "Switch", new { globalID = ViewBag.GlobalID, layerName = ViewBag.LayerName, layerType = ViewBag.layerType }, null)</li>
                            <li>@Html.ActionLink("Settings Files", "Files", "Switch", new { globalID = ViewBag.GlobalID, layerName = ViewBag.LayerName, layerType = ViewBag.layerType }, null)</li>
                            <li>@Html.ActionLink("History", "History", "Switch", new { globalID = ViewBag.GlobalID, layerName = ViewBag.LayerName, layerType = ViewBag.layerType }, null)</li>
                        </ul>
                    </nav>
                </div>
            </td>
        </tr>
        <tr>
            <td class="header" style="padding-top:15px; padding-bottom:0px; padding-left:10px">
              @if (ViewBag.layerType == "P")
                {
                    <label id="lablelayertype" runat="server" style="color: Red; font-size:16; text-align: Left;">
                       @ViewBag.Title 
                    </label>
                     
                   
                }
               else
               {
                @ViewBag.Title 
               }
            </td>
        </tr>
        <tr>
            <td style="padding: 10px;background-color:#ffffff">
                <table style="width:100%; background-color:#f2f2f2" cellpadding="0" cellspacing="0" >
                    <tr>
                        <td style="text-align:left; padding-bottom:0px">
                            <table style="width:100%" cellpadding="0" cellspacing="0">
                                @if (Model.GISAttributes.Count > 0)
                                {
                                    <tr>
                                        <td style="text-align: right; width:250px">
                                            @Html.DisplayFor(m => m.GISAttributes[0].DisplayName) :
                                        </td>
                                        <td>
                                            <label style="text-align: left">@Html.DisplayFor(m => m.GISAttributes[0].ValueDescription)</label>
                                        </td>
                                    </tr>
                                }
                            </table>
                        </td>
                    </tr>
                    <tr>
                        <td style="text-align:left; padding-top:0px">
                            <div id="divHistory" style="height: 400px; overflow: scroll; overflow-x: hidden;
                                width: 100%">
                                <table style="width:100%" cellpadding="0" cellspacing="0" >
                                    @for (int i = 1; i < Model.GISAttributes.Count; i++)
                                    {
                                 
                                        <tr>
                                            <td style="text-align: right;width:250px">
                                                @Html.DisplayFor(m => m.GISAttributes[i].DisplayName) :
                                            </td>
                                            <td>
                                                <label style="text-align: left">@Html.DisplayFor(m => m.GISAttributes[i].ValueDescription)</label>
                                            </td>
                                        </tr> 
                                    }
                                </table>
                            </div>
                        </td>
                    </tr>
                </table>
            </td>
        </tr>
    </table>
}