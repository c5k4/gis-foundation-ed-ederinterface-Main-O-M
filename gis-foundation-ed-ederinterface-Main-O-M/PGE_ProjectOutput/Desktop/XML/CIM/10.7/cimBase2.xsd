  <xs:complexType name="CimIec61968Version">
    <xs:annotation>
      <xs:documentation>This is the CIM version number assigned to this UML model file for all IEC 61968 packages.  For all other packages, refer to "CimVersion" in Main.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
