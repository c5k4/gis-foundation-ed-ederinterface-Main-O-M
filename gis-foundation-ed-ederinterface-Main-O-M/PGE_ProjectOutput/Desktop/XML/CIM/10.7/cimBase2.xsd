            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="impedanceH-X" type="cim:Impedance">
            <xs:annotation>
              <xs:documentation>Impedance Primary to Secondary.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="impedanceH-Y" type="cim:Impedance">
            <xs:annotation>
              <xs:documentation>Impedance Primary to Tertiary</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="impedanceX-Y" type="cim:Impedance">
