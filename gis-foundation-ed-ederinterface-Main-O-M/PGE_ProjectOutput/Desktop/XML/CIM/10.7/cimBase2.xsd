      <xs:documentation>Various cost items that are not associated with compatible units.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="cim:Naming">
        <xs:sequence>
          <xs:element minOccurs="0" name="quantity" type="cim:Numeric">
            <xs:annotation>
              <xs:documentation>The quantity of the misc. item being assigned to this location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="units" type="xs:string">
            <xs:annotation>
