          <xs:element minOccurs="0" name="costPerUnit" type="cim:Money">
            <xs:annotation>
              <xs:documentation>The cost per unit for this misc. item.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="costType" type="xs:string">
            <xs:annotation>
              <xs:documentation>The cost category for accounting, such as material, labor, vehicle, contractor, equipment, overhead.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="account" type="xs:string">
            <xs:annotation>
              <xs:documentation>This drives the accounting treatment for this misc. item.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
