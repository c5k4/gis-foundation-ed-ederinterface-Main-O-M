  <xs:complexType name="gml_Feature">
    <xs:annotation>
      <xs:documentation>Feature is used for rendering of an object. To facilitate the economic use of widely available graphical tools and applications, the Geography Markup Language (GML) is intended to be used for the management of graphical information.  GML is an XML grammar written in XML Schema for the modelling, transport, and storage of geographic information. It was created by the Open GIS Consortium and is standardized through ISO. </xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="value" type="xs:float" />
      <xs:element minOccurs="0" name="units" type="xs:string">
        <xs:annotation>
          <xs:documentation />
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="UserAttribute">
    <xs:annotation>
      <xs:documentation>This provides a mechanism for a user to specify additional attributes.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="Sequence" type="xs:string">
        <xs:annotation>
          <xs:documentation>The sequence number for this attribute in a list of attributes, starting with 1.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Name" type="xs:string">
        <xs:annotation>
          <xs:documentation>The name of an attribute.</xs:documentation>
        </xs:annotation>
      </xs:element>
      <xs:element minOccurs="0" name="Value" type="xs:string">
        <xs:annotation>
          <xs:documentation>The value of an attribute.</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="weight">
    <xs:annotation>
      <xs:documentation>The weight of an object.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="value" type="xs:float" />
      <xs:element minOccurs="0" name="units" type="xs:string">
        <xs:annotation>
          <xs:documentation>"Kilograms"</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:complexType name="Capacitance">
    <xs:annotation>
      <xs:documentation>A Farad, the typical unit, is the capacitance in which a charge of 1 coulomb produces 1 volt potential difference between its terminals.</xs:documentation>
    </xs:annotation>
    <xs:sequence>
      <xs:element minOccurs="0" name="value" type="xs:float" />
      <xs:element minOccurs="0" name="units" type="xs:string">
        <xs:annotation>
          <xs:documentation>"Pico Farads"</xs:documentation>
        </xs:annotation>
      </xs:element>
    </xs:sequence>
  </xs:complexType>
  <xs:simpleType name="CUActionCode">
    <xs:annotation>
      <xs:documentation>A list of valid activities to be performed on a Compatible Unit.</xs:documentation>
    </xs:annotation>
    <xs:restriction base="xs:string">
      <xs:enumeration value="toInstall" />
      <xs:enumeration value="toRemove" />
