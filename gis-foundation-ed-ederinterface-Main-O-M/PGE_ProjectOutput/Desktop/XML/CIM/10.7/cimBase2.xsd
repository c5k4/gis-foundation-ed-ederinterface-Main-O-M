  <xs:complexType name="TransformerObservations">
    <xs:annotation>
      <xs:documentation>Common information captured during transformer inspections and/or diagrnotics.  Note that some properties may be measured through other means and therefore have measurement values in addition to the observed values recorded here.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="cim:Naming">
        <xs:sequence>
          <xs:element minOccurs="0" name="dga" type="xs:string">
            <xs:annotation>
              <xs:documentation>Dissolved Gas Analysis.  Typical values are: Acceptable, Overheating, Corona, Sparking, Arcing.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="waterContent" type="xs:string">
            <xs:annotation>
              <xs:documentation>Water Content expressed in parts per million.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="oilColor" type="xs:string">
            <xs:annotation>
              <xs:documentation>Oil Quality Analysis-Color</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="oilDielectricStrength" type="cim:Voltage">
            <xs:annotation>
              <xs:documentation>Oil Quality Analysis-Dielectric Strength</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="oilNeutralizationNumber" type="xs:string">
            <xs:annotation>
              <xs:documentation>Oil Quality Analysis-Neutralization Number - Number - Mg KOH</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="oilIFT" type="xs:string">
            <xs:annotation>
              <xs:documentation>Oil Quality Analysis- inter facial tension (IFT) - number-Dynes/CM</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="hotSpotTemp" type="cim:Temperature">
            <xs:annotation>
              <xs:documentation>Hotspot oil temperature.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="topOilTemp" type="cim:Temperature">
            <xs:annotation>
              <xs:documentation>Top Oil Temperature</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="bushingTemp" type="cim:Temperature">
            <xs:annotation>
              <xs:documentation>Bushing temperature</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="pumpVibration" type="xs:string">
            <xs:annotation>
              <xs:documentation>Pump vibration, with typical values being: nominal, high.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="oilLevel" type="xs:string">
            <xs:annotation>
              <xs:documentation>The level of oil in the transformer.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="freqResp" type="xs:string">
            <xs:annotation>
              <xs:documentation>Frequency Response Analysis.  Typical values are: Acceptable, Slight Movement, Significant Movement, Failed,  Near Failure.  A graphic of the response diagram, which is a type of document, may be associated with this analysis through the recursive document relationship of the DataSet.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="furfuralDP" type="xs:string">
            <xs:annotation>
              <xs:documentation>Overall measure of furfural in oil and mechanical strength of paper. DP, the degree of polymerization, is the strength of the paper.  Furfural is a measure of furfural compounds, often expressed in parts per million.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
