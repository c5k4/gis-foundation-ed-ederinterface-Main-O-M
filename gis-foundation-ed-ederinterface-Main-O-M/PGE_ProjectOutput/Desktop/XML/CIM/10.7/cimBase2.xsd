  <xs:complexType name="DiagnosisDataSet">
    <xs:annotation>
      <xs:documentation>The result of a problem (typically an asset failure) diagnosis, which is a type of Procedure.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="cim:DataSet">
        <xs:sequence>
          <xs:element minOccurs="0" name="preliminaryCode" type="xs:string">
            <xs:annotation>
              <xs:documentation>Code for problem category determined during preliminary assessment.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="preliminaryRemarks" type="xs:string">
            <xs:annotation>
              <xs:documentation>Remarks pertaining to preliminary assessment of problem.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="preliminaryDate" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>Date and time preliminary assessment of problem was performed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="finalCode" type="xs:string">
            <xs:annotation>
              <xs:documentation>Code for diagnosed probem category.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="finalRemarks" type="xs:string">
            <xs:annotation>
              <xs:documentation>Remarks pertaining to findings during problem diagnosis.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="finalCause" type="xs:string">
            <xs:annotation>
              <xs:documentation>Cause of problem determined during diagnosis.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="finalOrigin" type="xs:string">
            <xs:annotation>
              <xs:documentation>Origin of problem determined during diagnosis.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="finalDate" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>Date and time diagnosis was completed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="rootCause" type="xs:string">
            <xs:annotation>
              <xs:documentation>Root cause of problem determined during diagnosis.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="rootOrigin" type="xs:string">
            <xs:annotation>
              <xs:documentation>Root origin of problem determined during diagnosis.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="rootRemarks" type="xs:string">
            <xs:annotation>
              <xs:documentation>Remarks pertaining to root cause findings during problem diagnosis.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="effect" type="xs:string">
            <xs:annotation>
              <xs:documentation>Effect of problem.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="phase" type="cim:PhaseCode">
            <xs:annotation>
              <xs:documentation>Phase(s) diagnosed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="failureMode" type="xs:string">
            <xs:annotation>
              <xs:documentation>Failuer mode, for example: Failure to Insulate; Failure to conduct; Failure to contain oil; Failure to provide ground plane; Other.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="TestDataSet">
    <xs:annotation>
      <xs:documentation>Test results, usually obtained by a lab or other independent organisation.</xs:documentation>
    </xs:annotation>
