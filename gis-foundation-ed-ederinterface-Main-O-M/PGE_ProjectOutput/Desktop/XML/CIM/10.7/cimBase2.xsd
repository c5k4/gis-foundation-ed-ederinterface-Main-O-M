    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="MeasurementValue.MeasurementValueQuality.MeasurementValueQuality.MeasurementValue">
    <xs:annotation>
      <xs:documentation>A MeasurementValue has a MeasurementValueQuality associated with it.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="mdimsg:SimpleAssociation" />
    </xs:complexContent>
  </xs:complexType>
