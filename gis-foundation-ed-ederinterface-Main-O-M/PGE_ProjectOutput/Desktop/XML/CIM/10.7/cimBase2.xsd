      <xs:enumeration value="asBuilt" />
    </xs:restriction>
  </xs:simpleType>
  <xs:complexType name="ElectronicAddress">
    <xs:annotation>
      <xs:documentation>Electronic location information.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="cim:Naming">
