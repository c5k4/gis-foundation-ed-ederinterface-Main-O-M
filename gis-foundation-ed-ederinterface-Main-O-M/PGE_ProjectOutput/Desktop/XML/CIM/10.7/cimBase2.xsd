        <xs:sequence>
          <xs:element minOccurs="0" name="insultationType" type="xs:string">
            <xs:annotation>
              <xs:documentation>Type of insulation used in bushing:Paper Oil, Compound, Solid Porcelain, other</xs:documentation>
            </xs:annotation>
