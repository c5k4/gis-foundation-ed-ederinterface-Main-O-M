            <xs:annotation>
              <xs:documentation>A codified representation of the location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="polygonFlag" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>Flag is true if the first and last point in the a sequence of coordinate pairs are to be connected, thus forming a polygon rather than merely a sequence of line segments.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="LocationGrant">
    <xs:annotation>
      <xs:documentation>A grant provides a right, as defined by type, for a parcel of land.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="cim:Document">
        <xs:sequence>
          <xs:element minOccurs="0" name="grantType" type="xs:string">
            <xs:annotation>
              <xs:documentation>Type of grant.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="signDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>Date that the Parcel Owner signed acknowledgement of the grant.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="status" type="xs:string">
            <xs:annotation>
              <xs:documentation>Status of the grant.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="grantDate" type="xs:date">
            <xs:annotation>
              <xs:documentation>Date that grant became official.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="propertyData" type="xs:string">
            <xs:annotation>
              <xs:documentation>Property related information that describes the Grant's land parcel.  For example, it may be a deed book number, deed book page number, and parcel number.</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="RightOfWay">
    <xs:annotation>
      <xs:documentation>A right-of-way (ROW) is for land where it is lawful to use for a public road, an electric power line, etc.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="cim:Document">
        <xs:sequence>
          <xs:element minOccurs="0" name="rightOfWayType" type="xs:string">
