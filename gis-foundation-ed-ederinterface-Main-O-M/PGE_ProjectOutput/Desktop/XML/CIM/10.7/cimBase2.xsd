  <xs:complexType name="DesignLocationCU">
    <xs:annotation>
      <xs:documentation>Compatible Unit at a given design location.</xs:documentation>
    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="cim:Naming">
        <xs:sequence>
          <xs:element minOccurs="0" name="complexityFactor" type="xs:string">
            <xs:annotation>
              <xs:documentation>A code that increases or decreases the labor component of a compatible unit based on perceived complexity.  Examples include sandy soil, rocky soil, back yard (vs. front yard), bad dogs, etc.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="cuQuantity" type="cim:Numeric">
            <xs:annotation>
              <xs:documentation>The quantity of the CU being assigned to this location.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="cuAction" type="xs:string">
            <xs:annotation>
              <xs:documentation>A code that drives instructs the crew what action to perform.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="cuUsage" type="xs:string">
            <xs:annotation>
              <xs:documentation>As the same CU can be used for different purposes and accounting purposes, usage must be specified.  Examples include: distribution, transmission, substation.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="cuAccount" type="xs:string">
            <xs:annotation>
              <xs:documentation>A code that helps direct accounting (capital, expense, or accounting treatment).</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="energizationFlag" type="xs:boolean">
            <xs:annotation>
              <xs:documentation>True if associated electrical equipment is intended to be energized while work is being performed.</xs:documentation>
            </xs:annotation>
          </xs:element>
          <xs:element minOccurs="0" name="yearRemoval" type="xs:dateTime">
            <xs:annotation>
              <xs:documentation>This is the year when a CU that represents an asset is removed</xs:documentation>
            </xs:annotation>
          </xs:element>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>
