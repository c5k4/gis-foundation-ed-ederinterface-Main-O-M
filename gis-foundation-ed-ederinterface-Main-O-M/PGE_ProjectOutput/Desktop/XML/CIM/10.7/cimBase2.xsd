    </xs:annotation>
    <xs:complexContent mixed="false">
      <xs:extension base="mdimsg:ComplexAssociation" />
    </xs:complexContent>
  </xs:complexType>
  <xs:complexType name="Limit.LimitSet.LimitSet.Limits">
